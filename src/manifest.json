{
  "manifest_version": 3,
  "name": "Dfinity Decoder",
  "devtools_page": "index.html",
  "permissions": [],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/icons.svg",
        "devtools/index.html",
        "sandbox/index.html"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "assets/16.png",
    "48": "assets/48.png",
    "128": "assets/128.png"
  },
  "sandbox": {
    "pages": ["sandbox/index.html"]
  },
  "content_security_policy": {
    "sandbox": "sandbox allow-scripts; script-src-elem  'unsafe-eval' 'unsafe-inline' 'self' https://* http://*; object-src 'self'"
  }
}
